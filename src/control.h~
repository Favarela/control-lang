#ifndef _CONTROL_H_
#define _CONTROL_H_

/* Definitions of manifest constants */
typedef enum {	WHILE, LPARENTHESES, RPARENTHESES, LCURLY, RCURLY, IF, ELSE,
		REPEAT, SEMICOLON, PAUSE, OUTSTRING, OUTINT, LCLICK, RCLICK,
		LRELEASE, RRLEASE, UP, DOWN, LEFT, RIGHT, PRESSKEY, RELEASEKEY,
		ID, EQASS, OR, AND, EQCOMP, NE, LT, LE, GT, GE, ADD, SUB,
		MUL, DIV, MOD, NEG, OPP, INTT, ININT, MOUSEPOSX, MOUSEPOSY,
		INSTRING, STRINGT
	     } TERMINALS;
	     
typedef enum {DOTO_} NON_TERMINALS;




#endif
